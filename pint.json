{
    "preset": "psr12",
    "exclude": [],
    "rules": {
        "no_superfluous_phpdoc_tags": true,
        "phpdoc_annotation_without_dot": true,
        "phpdoc_no_useless_inheritdoc": true,
        "phpdoc_trim": true,
        "concat_space": false,
        "nullable_type_declaration": {
            "syntax": "union"
        }
    }
}
